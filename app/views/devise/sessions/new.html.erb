<h2><%= t('.sign_in') %></h2>

<%= form_with model: resource, url: session_path(resource_name)  do |f| %>

  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit t('.sign_in') %>
  </div>

<% end %>

<%= render "devise/shared/links" %>
